{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.2.2",
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "mapbox-gl": "^2.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.11.5",
    "react-map-gl": "^5.2.10",
    "react-scripts": "^4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd server/src && npm install && node index.js",
    "build": "cd client  && npm run build",
